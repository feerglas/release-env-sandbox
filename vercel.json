{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "git": {
    "deploymentEnabled": {
      "main": false,
      "release-please--*": false
    }
  },
  "redirects": [
    {
      "source": "/storybook",
      "destination": "/storybook/"
    }
  ],
  "rewrites": [
    {
      "source": "/storybook",
      "destination": "/storybook"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "buildCommand": "npm run build && npm run build-storybook"
}